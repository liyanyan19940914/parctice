		var item=collection_a[i];
		if(is_Exit(item,collection_b)){
			arrary.push(item);
		}
	}
	return array;
}
	var is_Exit(element,collection){
		for(var j=0;j<collection_b.length;j++){
			if(collection[i]===element){
				return true;
			}
		}
